{
  "kind": "collectionType",
  "collectionName": "outbound_products",
  "info": {
    "singularName": "outbound-product",
    "pluralName": "outbound-products",
    "displayName": "Outbound_Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "out_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "qty": {
      "type": "integer"
    },
    "destination": {
      "type": "text"
    },
    "notes": {
      "type": "text"
    },
    "invoice": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    }
  }
}
