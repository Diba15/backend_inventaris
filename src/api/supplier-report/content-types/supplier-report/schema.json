{
  "kind": "collectionType",
  "collectionName": "supplier_reports",
  "info": {
    "singularName": "supplier-report",
    "pluralName": "supplier-reports",
    "displayName": "Supplier_Report"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "supplier_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::supplier.supplier"
    },
    "send_proof": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "shipping_date": {
      "type": "datetime"
    },
    "received_confirm": {
      "type": "boolean"
    }
  }
}
